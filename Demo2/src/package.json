{
  "name": "Demo2",
  "version": "1.0.0",
  "scripts": {
    "prestart": "yarn install --ignore-engines --pure-lockfile",
    "postinstall": "typings install",
    "client": "webpack-dev-server --inline --progress --port 8080",
    "server": "SET ASPNETCORE_ENVIRONMENT=Development && dotnet watch run",
    "build": "SET NODE_ENV=development && webpack -d --color && SET ASPNETCORE_ENVIRONMENT=Development && dotnet build",
    "start": "concurrently --kill-others \"npm run client\" \"npm run server\""
  },
  "dependencies": {
    "@angular/common": "~2.0.1",
    "@angular/compiler": "~2.0.1",
    "@angular/core": "~2.0.1",
    "@angular/forms": "~2.0.1",
    "@angular/http": "~2.0.1",
    "@angular/platform-browser": "~2.0.1",
    "@angular/platform-browser-dynamic": "~2.0.1",
    "@angular/router": "~3.0.1",
    "@angular/upgrade": "~2.0.1",
    "angular-in-memory-web-api": "~0.1.1",
    "bootstrap": "^3.3.7",
    "bootstrap-datepicker": "^1.6.4",
    "bootstrap-timepicker": "^0.5.2",
    "core-js": "^2.4.1",
    "jquery": "^3.1.0",
    "ng2-bootstrap": "^1.1.11",
    "ng2-datetime": "^1.2.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "ts-loader": "^0.8.2",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "aspnet-webpack": "^1.0.11",
    "concurrently": "^3.1.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "http-proxy-middleware": "^0.17.2",
    "ng-annotate-loader": "^0.2.0",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.5",
    "typescript": "^2.0.9",
    "typings": "^1.4.0",
    "url-loader": "^0.5.7",
    "webpack-dev-server": "^1.16.2",
    "webpack-fail-plugin": "^1.0.5",
    "webpack-hot-middleware": "^2.10.0"
  },
  "optionalDependencies": {
    "webpack": "1.13.2"
  }
}
